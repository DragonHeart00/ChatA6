<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chata6">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChatA6">
        <activity android:name=".message.MessageActivity"></activity>
        <activity
            android:name=".SignIn.LoginActivity"
            android:parentActivityName=".SignIn.StartActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SignIn.RegisterActivity"
            android:parentActivityName=".SignIn.StartActivity" />
        <activity android:name=".SignIn.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>